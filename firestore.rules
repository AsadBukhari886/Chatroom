rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /message/{userID} {
      allow read:if request.auth.uid!=null;
    allow write:if request.auth.uid!=null;
    }
     match/console/{userID}{
    allow read:if request.auth.uid!=null;
    allow write:if request.auth.uid!=null
    }
  }
}